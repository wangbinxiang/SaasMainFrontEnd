{
  "name": "saas-mian-frontend",
  "version": "0.2.0",
  "author": "wbx",
  "scripts": {
    "pm2": "npm-run-all --sequential compile start-pm2",
    "start-pm2": "pm2 start pm2.json",
    "start": "npm-run-all --sequential compile start-prod",
    "start-prod": "NODE_ENV=production node ./bin/production",
    "dev": "npm-run-all --sequential clean start-dev",
    "start-dev": "NODE_ENV=development nodemon --ignore webpack-assets.json --ignore client --ignore client-src --exec  PORT=3001 babel-node ./bin/development.js",
    "build": "npm test && npm run compile",
    "compile": "npm-run-all clean --parallel compile-server compile-client",
    "compile-server": "babel server-src/ --out-dir server/ --retain-lines --source-maps",
    "compile-client": "NODE_ENV=production webpack --colors --config ./webpack.config.js",
    "clean": "rimraf server && rimraf client/js/pages",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "engines": {
    "node": ">= 4"
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "co": "^4.6.0",
    "config": "^1.21.0",
    "debug": "^2.2.0",
    "ejs": "^2.4.1",
    "foundation-sites": "^6.2.3",
    "jquery": "^2.2.4",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^1.11.3",
    "koa-json": "^1.1.1",
    "koa-logger": "^1.3.0",
    "koa-memcached": "^0.1.2",
    "koa-multer": "^1.0.0",
    "koa-onerror": "^1.3.1",
    "koa-passport": "^2.2.2",
    "koa-router": "^7.0.0",
    "koa-static-plus": "^0.1.1",
    "koa-views": "^5.0.1",
    "lodash": "^4.11.1",
    "moment": "^2.14.1",
    "npm-run-all": "^3.0.0",
    "passport-local": "^1.0.0",
    "request-json": "^0.6.1",
    "underscore": "^1.8.3",
    "webpack-isomorphic-tools": "^2.5.8"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.1.4",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "chokidar": "^1.5.1",
    "colors": "^1.1.2",
    "debug": "^2.2.0",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.2.0",
    "eslint-plugin-promise": "^1.3.1",
    "eslint-plugin-standard": "^1.3.2",
    "isparta": "^4.0.0",
    "koa-webpack-middleware": "^1.0.1",
    "mocha": "^2.4.5",
    "nodemon": "^1.10.2",
    "rimraf": "^2.5.4",
    "should": "^8.3.0",
    "supertest": "^1.2.0",
    "webpack": "^1.13.2"
  },
  "repository": {},
  "license": "MIT"
}
